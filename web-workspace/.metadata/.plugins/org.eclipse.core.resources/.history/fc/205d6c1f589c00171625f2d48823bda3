<mapper namespace="kr.co.goodcard.dao.MemberDAO">
	<update id="insertMyCard1" parameterType="member">
		update MEMBER 
		set CARD1 = #{card1}
		where NO = #{no}
	</update>
	
	<update id="insertMyCard2" parameterType="member">
		update MEMBER 
		set CARD2 = #{card2}
		where NO = #{no}
	</update>
	
	<update id="insertMyCard3" parameterType="member">
		update MEMBER 
		set CARD3 = #{card3}
		where NO = #{no}
	</update>
</mapper>