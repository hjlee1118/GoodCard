<mapper namespace="kr.co.goodcard.dao.MemberDAO">
	<update id="insertMyCard" parameterType="member">
		update MEMBER 
		set CARD1 = #{card1}, CARD2 = #{card2}, CARD3 = #{card3} 
		where NO = #{no}
	</update>
</mapper>